<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5p %c{2} - %msg%n"/>
        </Console>
    </Appenders>
    <Loggers>
        <AsyncLogger name="TEST-DIGEST" additivity="false">
            <AppenderRef ref="Console"/>
        </AsyncLogger>

        <AsyncRoot level="INFO">
            <AppenderRef ref="Console"/>
        </AsyncRoot>

    </Loggers>
</Configuration>